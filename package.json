    "webpack-retry-chunk-load-plugin": "^3.1.1",
    "wrangler": "^3.103.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  },
  "lint-staged": {
    "*.scss": [
      "npm run format"
    ],
    "*.{vue,ts}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "bufferutil",
      "esbuild",
      "keccak",
      "sharp",
      "utf-8-validate",
      "vue-demi",
      "workerd"
    ]
  }
