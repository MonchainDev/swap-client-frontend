    "@wicchain/multicall": "0.0.4",
    "@wicchain/pcsx-sdk": "0.0.4",
    "@wicchain/permit2-sdk": "0.0.4",
    "@wicchain/pools": "0.0.4",
    "@wicchain/position-managers": "0.0.4",
    "@wicchain/price-api-sdk": "0.0.4",
    "@wicchain/routing-sdk": "0.0.4",
    "@wicchain/sdk": "0.0.4",
    "@wicchain/smart-router": "0.0.4",
    "@wicchain/stable-swap-sdk": "0.0.4",
    "@wicchain/swap-sdk-core": "0.0.4",
