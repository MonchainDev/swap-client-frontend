    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.5.2",
    "@tanstack/vue-query": "^5.66.0",
    "@vueuse/nuxt": "^12.7.0",
    "@wagmi/core": "^2.16.7",
    "@wagmi/vue": "^0.1.8",
    "apexcharts": "^4.5.0",
    "axios": "^1.8.1",
    "crypto-js": "^4.2.0",
    "d3": "^7.9.0",
    "decimal.js": "^10.4.3",
    "ethers": "^5.8.0",
    "graphql-request": "^7.1.2",
    "jsencrypt": "^3.3.2",
    "nuxt": "3.15.4",
    "nuxt-lodash": "^2.5.3",
    "nuxt-svgo": "^4.0.11",
    "pinia": "^2.2.0",
    "viem": "^2.23.5",
    "vue": "^3.4.34",
    "vue3-apexcharts": "^1.8.0",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/prompt-cli": "^19.3.1",
    "@nuxt/test-utils": "^3.14.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/crypto-js": "^4.2.2",
    "@types/d3": "^7.4.3",
    "@vitest/ui": "^2.0.5",
    "@vue/test-utils": "^2.4.6",
    "commitizen": "^4.3.0",
    "happy-dom": "^14.12.3",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "playwright-core": "^1.45.3",
